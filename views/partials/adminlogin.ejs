    <script type="text/javascript">
        $("#logout").on('click', function () {
            localStorage.clear();
        })
        var token = localStorage.getItem('token');
        check();
        function check() {
            $.ajax({
                url: 'token',
                type: 'get',
                headers: {
                    authorization: token
                },
                success: function (data) {
                    if (data.data != null) {
                        orgRetrive();
                    }
                    else {
                        window.location.href = '/login'
                    }
                }
            });
        }
        $("#create").on('click', function () {
            check();
            $('#organizationcard').show();
            $('#orgtablecard').hide();
        })
        $("#createproject").on('click', function () {
            check();
            var orgid = $('#projectcreatecard').val();
            console.log(orgid)
            $('#orgid1').val(orgid);
            $('#projectcreatecard').show();
            $('#repotablecard').hide();
        })
        $('#submitproject').on('click', function () {
            var form_data = $("#projectcreateform").serializeArray();
            console.log(form_data);
            var orgid = $('#repotablecard').val();
            $.ajaxSetup({
                beforeSend: function (xhr, settings) {
                    if (!/^(GET|HEAD|OPTIONS|TRACE)$/i.test(settings.type) && !this.crossDomain) {
                        xhr.setRequestHeader("X-CSRFToken", form_data._csrf)
                    }
                }
            });
            baseurls = "clone1"
            $.ajax({
                url: baseurls,
                type: 'post',
                headers: {
                    authorization: token
                },
                dataType: 'json',
                data: form_data,
                beforeSend: function () {
                    $('#loader').show();
                },
                complete: function () {
                    $('#loader').hide();
                },
                success: function (data) {
                    if (data.status != null) {
                        if (data.status.status == "ok") {
                            alert(data.status.message)
                            $('#projectcreatecard').hide();
                            $('#repotablecard').show();
                            repoRetrive(orgid)
                        }
                        else {
                            alert(data.status.message)

                        }
                    }
                    else {
                        window.location.href = '/login'
                    }
                }
            });
        })
        $("#createuser").on('click', function () {
            check();
            $('#usercard').show();
            var orgid = $('#repotablecard').val();
            $('#orgid').val(orgid);
            $('#usertablecard').hide();
        })
        $('#submituser').on('click', function () {
            var form_data = $("#userform").serializeArray();
            console.log(form_data)
            var orgid = $('#repotablecard').val();
            $.ajaxSetup({
                beforeSend: function (xhr, settings) {
                    if (!/^(GET|HEAD|OPTIONS|TRACE)$/i.test(settings.type) && !this.crossDomain) {
                        xhr.setRequestHeader("X-CSRFToken", form_data._csrf)
                    }
                }
            });
            baseurls = "adminorguserreg"
            $.ajax({
                url: baseurls,
                type: 'post',
                headers: {
                    authorization: token
                },
                dataType: 'json',
                data: form_data,
                success: function (data) {
                    if (data.data != null) {
                        alert('User created successfully')
                        $('#usercard').hide();
                        $('#usertablecard').show();
                        orguserRetrive(orgid)
                    }
                    else {
                        window.location.href = '/login'
                    }
                }
            });
        })
        $(document).ready(function () {
            $('#Adminname').empty()
            $('#Adminname').append('Admin')
            orgRetrive();
            $('#sidebarCollapse').on('click', function () {
                $('#sidebar').toggleClass('active');
            });
        });
        $('#submit').on('click', function () {
            var form_data = $("#organizationform").serializeArray();
            console.log(form_data);
            $.ajaxSetup({
                beforeSend: function (xhr, settings) {
                    if (!/^(GET|HEAD|OPTIONS|TRACE)$/i.test(settings.type) && !this.crossDomain) {
                        xhr.setRequestHeader("X-CSRFToken", form_data._csrf)
                    }
                }
            });
            baseurls = "orgreg"
            $.ajax({
                url: baseurls,
                type: 'post',
                headers: {
                    authorization: token
                },
                dataType: 'json',
                data: form_data,
                success: function (data) {
                    if (data.data != null) {
                        alert('orgcreated successfully')
                        $('#organizationcard').hide();
                        $('#orgtablecard').show();
                        orgRetrive();
                    }
                    else {
                        window.location.href = '/login'
                    }
                }
            });
        })
        function orgRetrive() {
            $.ajaxSetup({
                beforeSend: function (xhr, settings) {
                    if (!/^(GET|HEAD|OPTIONS|TRACE)$/i.test(settings.type) && !this.crossDomain) {
                        xhr.setRequestHeader("X-CSRFToken", csrftoken)
                    }
                }
            });
            var ses = "SPADMIN";
            $('#orgtbl').DataTable({
                "serverSide": false,
                "fixedheader": true,
                "language": {
                    search: "_INPUT_",
                    searchPlaceholder: "Search Here ..",
                    processing: '<i class="fa fa-spinner fa-pulse " ></i>'
                },
                "destroy": true,
                "searching": true,
                "ordering": false,
                "scrollX": true,
                "processing": true,
                "ajax": {
                    "url": "orgtable",
                    "type": 'GET',
                    "headers": { authorization: token },
                    dataSrc: function (json) {
